{
  "manifest_version": 3,
  "name": "指标命名助手",
  "version": "1.0.0",
  "description": "中文转英文变量命名工具",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "clipboardWrite"
  ]
}