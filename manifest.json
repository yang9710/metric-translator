{
  "manifest_version": 3,
  "name": "TransFluent",
  "version": "1.0.0",
  "description": "中文转英文变量命名工具",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "vite.svg",
      "48": "vite.svg",
      "128": "vite.svg"
    }
  },
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  },
  "permissions": [
    "clipboardWrite"
  ]
}